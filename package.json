{
  "name": "lodm",
  "version": "0.0.1",
  "description": "LOD Mesh Library",
  "main": "./index.js",
  "author": "aninggo <aninggo@gmail.com>",
  "scripts": {
    "dev": "node bootstrap --env.NODE_ENV=dev",
    "build:dev": "webpack --env.NODE_ENV=dev --progress --color",
    "build": "webpack --env.NODE_ENV=prod --progress --color",
    "prebuild": "run-s clean lint",
    "clean": "rimraf build",
    "lint": "eslint src/**",
    "lint-fix": "eslint src/** --fix",
    "test": "mocha test/**/*.test.js --require @babel/register && npm run lint && npm run build"
  },
  "keywords": [
    "lodm",
    "threejs",
    "3d"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.19.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "expect.js": "^0.3.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.3",
    "serve-static": "^1.13.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.4",
    "worker-loader-es6": "^2.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "color": "^3.1.1",
    "three": "^0.86.0",
    "three-trackballcontrols": "0.0.7"
  }
}
